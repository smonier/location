<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.jahia.services.content.rules.ModuleGlobalObject">
        <property name="globalRulesObject">
            <map>
                <entry key="locationService">
                    <bean class="org.jahia.modules.location.LocationService">
                    </bean>
                </entry>
            </map>
        </property>
    </bean>

    <bean class="org.jahia.modules.location.MapAttributesFilter">
        <property name="priority" value="31"/>
        <property name="description" value="Location module: Inject maps parameter in request to be used by map .jsp"/>
        <property name="googleMapApiKey" value="${jahia.modules.location.attr.map.key:}"/>
        <property name="applyOnNodeTypes" value="jnt:map,jnt:staticMap"/>
    </bean>
</beans>